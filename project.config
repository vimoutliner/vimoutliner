[project]
	description = Work fast, think well.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Matěj Cepl (mcepl)
[access "refs/for/refs/*"]
	read = group user/Matěj Cepl (mcepl)
	push = group user/Matěj Cepl (mcepl)
	label-Code-Review = -2..+2 group user/Matěj Cepl (mcepl)
	label-Verified = -1..+1 group user/Matěj Cepl (mcepl)
	rebase = group user/Matěj Cepl (mcepl)
	removeReviewer = group user/Matěj Cepl (mcepl)
	submit = group user/Matěj Cepl (mcepl)
[access "refs/heads/*"]
	read = group user/Matěj Cepl (mcepl)
	create = group user/Matěj Cepl (mcepl)
	push = +force group user/Matěj Cepl (mcepl)
	pushMerge = group user/Matěj Cepl (mcepl)
[access "refs/tags/*"]
	push = +force group user/Matěj Cepl (mcepl)
	pushTag = group user/Matěj Cepl (mcepl)
	pushSignedTag = group user/Matěj Cepl (mcepl)
